pipeline {
    agent any

    stages {
      stage('SCM Checkout') {
        steps {
            git 'https://github.com/Romok1/ansible-config-mgt.git'
        }
      }
      stage('Execute Ansible') {
        steps {
            ansiblePlaybook credentialsId: 'private-key', disableHostKeyChecking: true, installation: 'Ansiblejenk', inventory: 'inventory/dev', playbook: 'playbooks/site.yml'
        }
      }
    }
}   
