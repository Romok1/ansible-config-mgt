        script {
          sh 'echo "Building Stage"'
        }
      }
    }

    stage('Test') {
      steps {
        script {
          sh 'echo "Testing Stage"'
        }
      }
    }
    stages {
    stage('Package') {
      steps {
        script {
          sh 'echo "Package Stage"'
        }
      }
    }

    stage('Deploy') {
      steps {
        script {
          sh 'echo "Deployin Stage"'
        }
      }
    }
    stages {
    stage('Clean up') {
      steps {
        script {
          sh 'echo "cleaning up Stage"'
        }
      }
    }
    }
}